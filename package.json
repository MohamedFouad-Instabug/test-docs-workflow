{
  "name": "recipehub",
  "version": "2.0.0",
  "description": "A modern recipe management platform with user authentication, ratings, social features, and dietary preferences",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "react-router-dom": "^6.8.0",
    "axios": "^1.3.0",
    "jwt-decode": "^3.1.2",
    "react-hook-form": "^7.43.0",
    "react-query": "^3.39.3",
    "react-hot-toast": "^2.4.0",
    "framer-motion": "^10.0.0",
    "react-intersection-observer": "^9.4.3",
    "react-virtualized": "^9.22.3",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "clsx": "^1.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "type-check": "npx tsc --noEmit",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --watchAll=false --coverage --ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MohamedFouad-Instabug/test-docs-workflow.git"
  },
  "keywords": [
    "react",
    "recipes",
    "food",
    "cooking",
    "authentication",
    "social",
    "ratings",
    "dietary-preferences",
    "modern-ui"
  ],
  "author": "RecipeHub Team",
  "license": "MIT",
  "homepage": "https://recipehub.example.com"
}
